{
    "name": "webit/message-bus-sf-process",
    "type": "library",
    "keywords": ["message bus", "message queue", "process", "command", "symfony"],
    "authors": [{
        "name": "Daniel Bojdo",
        "email": "daniel.bojdo@web-it.eu"
    }],
    "license": "MIT",
    "require": {
        "php": ">=7.0",
        "webit/message-bus": "^1.0.0",
        "symfony/process": "^3.0|^4.0"
    },
    "autoload": {
        "psr-4": {
            "Webit\\MessageBus\\Infrastructure\\Symfony\\Process\\": "src/"
        }
    },
    "provide": {
        "webit/message-bus-infrastructure": "1.0.0"
    },
    "require-dev": {
        "phpspec/phpspec": "^4.2.0",
        "webit/test-tools": "^1.0.0",
        "mockery/mockery": "^1.0"
    },
    "suggest": {
        "symfony/console": "To use console commands for Utils and Listeners"
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\Webit\\MessageBus\\Infrastructure\\Symfony\\Process\\": "spec/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
